{
  "name": "xma-extension",
  "displayName": "Xma extension",
  "version": "0.0.1",
  "description": "xma's extension",
  "author": "xma",
  "scripts": {
    "dev": "rm -rf build/* && plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "@douyinfe/semi-ui": "^2.49.1",
    "ahooks": "^3.7.8",
    "lodash": "^4.17.21",
    "plasmo": "0.88.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "~resources/*"
        ]
      }
    ],
    "permissions": [
      "storage",
      "tabs"
    ]
  }
}
